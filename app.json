{
  "expo": {
    "name": "VCA Paspoort NL",
    "slug": "vcapaspoort-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "vcapaspoort",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#06060f"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "nl.vcapaspoort.app",
      "appleTeamId": "W67P88JWQA",
      "infoPlist": {
        "NSCameraUsageDescription": "Om certificaat-foto's te maken",
        "NSPhotoLibraryUsageDescription": "Om certificaat-foto's te uploaden"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#06060f"
      },
      "package": "nl.vcapaspoort.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "versionCode": 8
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-camera",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#3b82f6",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9baa43d9-d05f-42a1-98a9-fd8221f4489d"
      }
    },
    "owner": "handsomebstrd"
  }
}
